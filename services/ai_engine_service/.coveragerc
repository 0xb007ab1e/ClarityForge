[run]
source = .
omit = 
    */venv/*
    */virtualenv/*
    */.venv/*
    */env/*
    */.env/*
    */site-packages/*
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    setup.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ .__main__.:
    class .*\bProtocol\b.*\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
